{
  "name": "otona-eng-local",
  "version": "1.0.0",
  "main": "dist/main.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "test": "jest --setupFilesAfterEnv ./jest.setup.js",
    "dev": "concurrently \"npm:dev:renderer\" \"npm:dev:electron\"",
    "dev:renderer": "vite",
    "dev:electron": "wait-on http://localhost:5173 && electron dist/main.js",
    "build": "tsc && npm run build:renderer",
    "build:css": "tailwindcss -i ./src/index.css -o ./public/output.css --minify",
    "build:renderer": "vite build",
    "postbuild": "cp src/preload.js dist/preload.js",
    "start": "electron dist/main.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.14.1",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vercel/node": "^5.1.14",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.1.2",
    "electron": "^35.2.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.5.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tailwindcss": "^3.3.3",
    "ts-jest": "^29.3.2",
    "typescript": "^5.8.3",
    "vite": "^5.2.10",
    "wait-on": "^8.0.3"
  },
  "dependencies": {
    "dotenv": "^16.5.0",
    "firebase": "^11.6.0",
    "jsonrepair": "^3.12.0",
    "node-fetch": "^3.3.2"
  }
}
